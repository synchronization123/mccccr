<body>

<div class="sidebar">
    <a href="#" onclick="loadContent('patch')">Patch</a>
    <a href="#" onclick="loadContent('crm')">CRM</a>
    <a href="#" onclick="loadContent('cst')">CST</a>
    <a href="#" onclick="loadContent('stt')">STT</a>
    <a href="#" onclick="loadContent('rdt')">RDT</a>
    <a href="#" onclick="loadContent('hhj')">HHJ</a>
    <a href="#" onclick="loadContent('wss')">WSS</a>
</div>

<div class="content">
    <iframe id="mainContent" src="" frameborder="0"></iframe>
</div>

<script>
    const urls = {
        'patch': 'https://example.com/patch',
        'crm': 'https://example.com/crm',
        'cst': 'https://example.com/cst',
        'stt': 'https://example.com/stt',
        'rdt': 'https://example.com/rdt',
        'hhj': 'https://example.com/hhj',
        'wss': 'https://example.com/wss'
    };

    function loadContent(tab) {
        const iframe = document.getElementById('mainContent');
        iframe.src = urls[tab];
    }
</script>

</body>
</html>