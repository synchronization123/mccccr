import requests

# API details
API_URL = "https://dependencytrackapi.crm.com/api/v1/project/jsdjjdjdjd"
API_TOKEN = "bzbdbdhdhd"
HEADERS = {
    "X-Api-Key": API_TOKEN,
    "Content-Type": "application/json"
}

def fetch_dependency_data():
    try:
        response = requests.get(API_URL, headers=HEADERS)
        print(f"Response Status: {response.status_code}")
        print(f"Response Headers: {response.headers}")
        print(f"Response Body: {response.text}")  # Print full API response for debugging

        response.raise_for_status()  # Raises error for 4xx and 5xx status codes
        return response.json()
    except requests.exceptions.RequestException as e:
        print(f"Error fetching data: {e}")
        return None

data = fetch_dependency_data()